<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/btn_udarawanasinghe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UdaraWanasinghe"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_highjune"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HighJune"
        app:layout_constraintBottom_toTopOf="@id/btn_udarawanasinghe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_my_custom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MyCustom"
        app:layout_constraintBottom_toTopOf="@id/btn_highjune"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>